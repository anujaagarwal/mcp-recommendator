{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "ai-sdk/*": ["../node_modules/ai-sdk/*"],
      "bee-framework/*": ["../node_modules/bee-framework/*"],
      "public/*": ["./public/*"],
      "typeRoots": ["./node_modules/@types"]
      
    },
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/index.tsx", "app/index.jsx", "app/index.jsx"],
  "exclude": ["../node_modules"]
}
